<odoo>
    <data> 
        <record id="rds_extention_download_view" model="ir.ui.view">
	        <field name="name">rds.extention.download.view</field>
	        <field name="model">rds_extention_download</field>
	        <field name="type">form</field>
	        <field name="arch" type="xml">
		      <form string="Download">
		          <group>
		          	<field name="file_name"/>
				    <field name="fileStorage" readonly="1" filename="file_name"/>
				  </group>
				 <footer>
                    <button special="cancel" string="Close"/>
                 </footer>
		      </form>
		    </field>
	    </record>
	    
        <record id="rds_extention_upload_view" model="ir.ui.view">
            <field name="name">rds.extention.upload.view</field>
            <field name="model">rds_extention_upload</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
              <form string="Upload">
                  <group>
                    <field name="fileStorage"/>
                  </group>
                 <footer>
                    <button name="inport_project" string="Upload" type="object"/>
                    <button special="cancel" string="Close"/>
                 </footer>
              </form>
            </field>
        </record>
    </data>
</odoo>