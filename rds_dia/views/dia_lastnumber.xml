<?xml version="1.0" ?>
<odoo>
	<data>
	
    <record id="rds_dia_lastnumber_form_view" model="ir.ui.view">
        <field name="name">numeratori_dia_form</field>
        <field name="model">dia.lastnumber</field>
        <field name="priority" eval="2000"/>
        <field name="arch" type="xml">
            <form >
            	<group>
	                <field name="key"/>
	                <field name="last_number"/>
	                <field name="create_uid" readonly="1"/>
	                <field name="create_date" readonly="1"/>
	           	</group>
            </form>
        </field>
    </record>

    <record id="rds_dia_lastnumber_tree_view" model="ir.ui.view">
        <field name="name">numeratori_dia_tree</field>
        <field name="model">dia.lastnumber</field>
        <field name="arch" type="xml">
            <tree >
                <field name="key"/>
                <field name="last_number"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="rds_action_numeratori_dia">
        <field name="name">Numeratori Dia</field>
         <field name="res_model">dia.lastnumber</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    <menuitem 
        name="Numeratori Dia" 
        action="rds_action_numeratori_dia" 
        id="rds_menu_numeratori_dia" 
        parent="omnia_ddt.menu_action_DDT"
        sequence="12" 
    />

    </data>
</odoo>
