<odoo>
<!-- Main template -->
<template id="report_plan_manufactory_order_report">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-call="rds_extension.report_plan_manufactory_order_report_single" t-lang="doc.partner_id.lang"/>
        </t>
    </t>
</template>

<!-- Translatable template -->
<template id="report_plan_manufactory_order_report_single">
    <!-- Re-browse of the record with the partner lang -->
    <t t-set="doc" t-value="doc.with_context({'lang':doc.partner_id.lang})" />
    <t t-call="web.external_layout">
        <div class="page">
            <div class="oe_structure"/>
            <t t-raw="doc.getReportQtyWhere()"/>
        </div>
    </t>
</template>
</odoo>