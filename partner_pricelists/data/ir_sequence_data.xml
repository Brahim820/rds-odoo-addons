<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_rds_pricelist" model="ir.sequence">
            <field name="name">Partner Pricelist Offer</field>
            <field name="code">partner.pricelist.offer</field>
            <field name="prefix">PLO-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
                
        <record id="seq_rds_pricelist" model="ir.sequence">
            <field name="name">Partner Pricelists</field>
            <field name="code">partner.pricelist</field>
            <field name="prefix">LIS-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>