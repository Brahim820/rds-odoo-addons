<?xml version="1.0" encoding="utf-8"?>
<odoo><data>
  <record model="ir.ui.view" id="rds_extension_stock_view_inventory_form">
      <field name="name">rds.extension.stock.view.inventory.form</field>
      <field name="model">stock.inventory</field>
      <field name="inherit_id" ref="stock.view_inventory_form"/>
      <field name="arch" type="xml"> 
          <xpath expr="//tree//field[@name='product_qty']" position="after">
              <field name="remain_qty"/>
              <field name="note"/>
          </xpath>
      </field>
  </record>
</data></odoo>
