#. module: partner_pricelists
#: model:mail.template,body_html:partner_pricelists.email_template_partner_pricelist
msgid ""
"\n"
"<p>Dear ${object.partner_id.name}\n"
"% set doc_name = 'Pricelist Offer' if object.state in ('draft', 'sent') else 'Pricelist Confirmation'\n"
"\n"
"% if object.partner_id.parent_id:\n"
"    (<i>${object.partner_id.parent_id.name}</i>)\n"
"% endif\n"
",</p>\n"
"<p>\n"
"Here is the ${doc_name} <strong>${object.name}</strong>\n"
"% if object.origin:\n"
"(with reference: ${object.origin} )\n"
"% endif\n"
"from ${object.company_id.name}.\n"
"</p>\n"
"\n"
"<p>You can reply to this email if you have any questions.</p>\n"
"<p>Thank you,</p>\n"
"\n"
"<p style=\"color:#888888;\">\n"
"% if object.user_id and object.user_id.signature:\n"
"    ${object.user_id.signature | safe}\n"
"% endif\n"
"</p>\n"
msgstr ""
"\n"
"<p>Gentile ${object.partner_id.name}\n"
"% set doc_name = 'l'Offerta di Listino' if object.state in ('draft', 'sent') else 'il Listino'\n"
"\n"
"% if object.partner_id.parent_id:\n"
"    (<i>${object.partner_id.parent_id.name}</i>)\n"
"% endif\n"
",</p>\n"
"<p>\n"
"Può trovare allegato ${doc_name} <strong>${object.name}</strong>\n"
"% if object.origin:\n"
"(doc. di riferimento: ${object.origin} )\n"
"% endif\n"
"da ${object.company_id.name}.\n"
"</p>\n"
"\n"
"<p>Può rispondere a questa email per qualsiasi domanda.</p>\n"
"<p>Ringraziando,</p>\n"
"\n"
"<p style=\"color:#888888;\">\n"
"% if object.user_id and object.user_id.signature:\n"
"    ${object.user_id.signature | safe}\n"
"% endif\n"
"</p>\n"

