<?xml version="1.0" ?>
<odoo>

    <record id="rds_workcenter_info" model="ir.ui.view">
        <field name="name">rds.product.category.form</field>
        <field name="model">mrp.workcenter</field>
        <field name="priority" eval="2000"/>
        <field name="inherit_id" ref="mrp.mrp_workcenter_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='capacity']/.." position="inside">
                <group string="Dislocazione">
                    <field name="department_id"/>
                    <field name="location"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>