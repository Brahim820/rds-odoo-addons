<odoo>
	<data>
		<record id="oee_gantt_view" model="ir.ui.view">
				<field name="name">mrp.workcenter.productivity.gantt</field>
				<field name="model">mrp.workcenter.productivity</field>
				<field name="priority">20</field>
				<field name="arch" type="xml">
					<gantt type="consolidate"
						date_start="date_start"
						date_stop="date_end"
						fold_last_level="true"
						round_dnd_dates="true"
						consolidation="duration"
						string="Minute(s)"
					/>
				</field>
		</record>

		<record id="omnia_machine_updater_mrp_workcenter_view" model="ir.ui.view">
			<field name="name">omnia.machine.updater.mrp.workcenter.view</field>
			<field name="model">mrp.workcenter</field>
			<field name="inherit_id" ref="mrp.mrp_workcenter_view"/>
			<field name="arch" type="xml">
				<xpath expr="//notebook//page[last()]" position="after">
					<page string="Configurazione Macchina" groups="base.group_no_one">
					<group col="2">
						<button name="updateMachineLog" string="GetLog" type="object"/>
						<button name="getDeleteMachineLogParsered" string="Generare MWP" type="object"/>
						<button name="getUpdateLog" string="GetLog Generare MWP" type="object"/>
						<field name="machine_path"/>
						<field name="machine_user"/>
						<field name="machine_password" password="True"/>
					</group>
					<field name="machine_log"/>
					</page>
				</xpath>
				</field>
		</record>
	</data>

	 <data noupdate="1"> 
	 
		<record id="rds_check_machine_working_morning" model="ir.cron">
		    <field name="name">Check Machine Working Morning</field>
		    <field name="active" eval="False"/>
		    <field name="state">code</field>
		    <field name="interval_number">24</field>
		    <field name="interval_type">hours</field>
		    <field name="numbercall">-1</field>
		    <field name="doall" eval="False"/>
		    <field name="model_id" ref="mrp.model_mrp_workcenter"/>
		    <field name="code">env['mrp.workcenter'].check_machine_electronic_boards('mail_from', 'mail_to')</field>
		    <field name="args">()</field>
		</record> 
	
		<record id="rds_check_machine_working_evening" model="ir.cron">
		    <field name="name">Check Machine Working Evening</field>
		    <field name="active" eval="False"/>
		    <field name="state">code</field>
		    <field name="interval_number">24</field>
		    <field name="interval_type">hours</field>
		    <field name="numbercall">-1</field>
		    <field name="doall" eval="False"/>
		    <field name="model_id" ref="mrp.model_mrp_workcenter"/>
		    <field name="code">env['mrp.workcenter'].check_machine_electronic_boards('mail_from', 'mail_to')</field>
		    <field name="args">()</field>
		</record> 
	</data>
</odoo>
